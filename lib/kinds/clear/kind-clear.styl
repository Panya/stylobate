kind-clear()
  if !ie
    stylobate_defaults(arguments,
      -before (after in arguments ? '' : '&:before'),
      -after (before in arguments ? '' : '&:after'))

    deprecate(arguments, after)
    deprecate(arguments, before)

  zoom: 1

kind-clear-before()
  @extends $kind-clear-before

kind-clear-after()
  @extends $kind-clear-after

$kind-clear-before
  content: ""
  display: table

$kind-clear-after
  content: ""
  display: table
  clear: both
