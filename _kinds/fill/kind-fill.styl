kind-fill()
  position: retrieve(arguments, fixed static sticky, absolute)

  $sides = get_sides(arguments)
  top: $sides[0]
  right: $sides[1]
  bottom: $sides[2]
  left: $sides[3]

  $bg = get_string(arguments)
  $bg_color = get_color(arguments)
  if $bg
    $bg_color = unquote('') unless $bg_color
    background: $bg_color url($bg) retrieve(arguments, repeat repeat-x repeat-y , no-repeat) 50%
    // How to add bgposition here?
  else if $bg_color
    background: $bg_color

